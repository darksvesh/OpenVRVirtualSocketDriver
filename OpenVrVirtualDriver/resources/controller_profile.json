{
  "jsonid": "input_profile",
  "controller_type": "Creepybox Virtual controller",
  "legacy_binding" : "legacy_bindings.json",
  "device_class" : "TrackedDeviceClass_Controller",
  "input_bindingui_mode": "single_device",
  "input_bindingui_right": {
		"image": "game_controller.svg"
  },
  "should_show_binding_errors": true,
  "input_source": {
    "/input/joystick": {
      "type": "joystick",
      "click": false,
      "touch": false,
      "binding_image_point": [ 0, 0 ]
    },
    "/input/trackpad": {
      "type": "trackpad",
      "binding_image_point": [ 0, 0 ],
      "force": false,
      "click": false,
      "touch": false
    },
    "/input/trigger/value": {
      "type": "trigger",
      "binding_image_point": [ 0, 0 ]
    },
    "/input/trigger/squeeze": {
      "type": "trigger",
      "binding_image_point": [ 0, 0 ]
    },
    "/input/a/click": {
      "type": "button",
      "binding_image_point": [ 0, 0 ]
    },
    "/input/b/click": {
      "type": "button",
      "binding_image_point": [ 0, 0 ]
    },
    "/input/x/click": {
      "type": "button",
      "binding_image_point": [ 0, 0 ]
    },
    "/input/y/click": {
      "type": "button",
      "binding_image_point": [ 0, 0 ]
    }
  }
}